{
  "name": "nuxeo/nuxeo-automation-php-client",
  "description": "PHP Client Library for the Nuxeo Automation API",
  "keywords": ["nuxeo", "automation"],
  "homepage": "http://www.nuxeo.com",
  "license": "AL2",
  "authors": [
    {
      "name": "Pierre-Gildas MILLON",
      "email": "pgmillon@nuxeo.com"
    }
  ],
  "extra": {
    "branch-alias": {
      "dev-master": "1.5-dev"
    }
  },
  "require": {
    "php": ">=5.3.2",
    "guzzlehttp/guzzle": "3.8.*",
    "jms/serializer": "0.16.*",
    "zendframework/zend-http": "2.4.*"
  },
  "require-dev": {
    "phpunit/phpunit": "4.8.*",
    "symfony/http-foundation": "3.1.*"
  },
  "autoload": {
    "psr-4": { "Nuxeo\\": "src/Nuxeo" }
  },
  "autoload-dev" : {
    "psr-4": { "Nuxeo\\Tests\\" : "tests/Nuxeo/Tests" }
  }
}